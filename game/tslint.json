{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "no-console": [true, "log"],
        "indent": [true, "tabs", 2],
        "trailing-comma": false,
        "arrow-parens": false
    },
    "rulesDirectory": []
}